{
  "compilerOptions": {
    "resolveJsonModule": true,
    "strictPropertyInitialization": true,
    "outDir": "dist",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": "./",
    "paths": {
      "~game*": ["game*"],
      "~services*": ["services/"]
    },
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true
  },
}
